<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.controller.UserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>A</servlet-name>
  	<display-name>A</display-name>
  	<description></description>
  	<servlet-class>com.controller.A</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserController2</servlet-name>
  	<display-name>UserController2</display-name>
  	<description></description>
  	<servlet-class>com.controller.UserController2</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/UserController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>A</servlet-name>
  	<url-pattern>/A</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserController2</servlet-name>
  	<url-pattern>/UserController2</url-pattern>
  </servlet-mapping>
</web-app>
